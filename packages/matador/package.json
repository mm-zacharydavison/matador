{
  "name": "@zdavison/matador",
  "version": "2.0.1",
  "description": "A batteries-included event system.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "bin": {
    "matador": "./cli.ts"
  },
  "scripts": {
    "build": "rm -rf dist *.tsbuildinfo && tsup && tsc --build && cp dist/index.d.ts dist/index.d.cts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:integration": "bun test --grep integration",
    "lint": "bunx biome check .",
    "lint:fix": "bunx biome check . --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "cli": "bun ./cli.ts",
    "send-test-event": "bun ./cli.ts ./examples/config.ts ./examples/event.ts"
  },
  "dependencies": {
    "amqplib": "^0.10.4"
  },
  "devDependencies": {
    "@testcontainers/rabbitmq": "^10.16.0",
    "@types/amqplib": "^0.10.5",
    "testcontainers": "^10.16.0",
    "tsup": "^8.3.5"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "events",
    "messaging",
    "queue",
    "rabbitmq",
    "bullmq",
    "kafka",
    "transport"
  ],
  "license": "MIT"
}
