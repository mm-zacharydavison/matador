{
  "name": "@zdavison/matador-nest",
  "version": "2.0.10",
  "description": "NestJS integration for Matador event processing library",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./testing": {
      "import": {
        "types": "./dist/testing.d.ts",
        "default": "./dist/testing.js"
      },
      "require": {
        "types": "./dist/testing.d.cts",
        "default": "./dist/testing.cjs"
      }
    }
  },
  "scripts": {
    "build": "rm -rf dist *.tsbuildinfo && tsup && tsc --build && cp dist/index.d.ts dist/index.d.cts && cp dist/testing.d.ts dist/testing.d.cts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "bunx biome check .",
    "lint:fix": "bunx biome check . --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "@zdavison/matador": "^2.0.10",
    "reflect-metadata": "^0.1.13 || ^0.2.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/core": "^1.15.3",
    "@zdavison/matador": "2.0.10",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.0.0",
    "tsup": "^8.3.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "nestjs",
    "events",
    "messaging",
    "queue",
    "rabbitmq",
    "matador"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ]
}
